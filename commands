alias a='eval $(cat $DOTFILE_DIR/lists/applications.txt | fzf)'					# APPLICACTIONS
alias d='eval $(cat $DOTFILE_DIR/lists/docker.txt | fzf)'						# DOCKER
alias de='cd ~/Desktop'												            # GO TO DESKTOP
alias dd='cd ~/Documents'                                                       # GO TO DOCUMENTS
alias c='clear'													                # CLEAR TERMINAL
alias ff='vim $(find . -type d \( -name node_modules -o -name .angular -o -name target -o -name .git \) -prune -o -type f ! -name "*.png" | fzf --preview="bat --color=always {}")' # OPEN FILE 
alias h='cd ~/Developer'											            # GO TO DEVELOPER DIR	
alias hh='eval $(cat ~/.zsh_history | fzf)'                                     # HISTORY
alias kk='eval $(cat $DOTFILE_DIR/lists/kubernetes.txt | fzf)'					# KUBERNETES 
alias i='eval $(cat $DOTFILE_DIR/lists/terraform.txt | fzf)'					# TERRAFORM
alias ii='eval $(cat $DOTFILE_DIR/lists/information.txt | fzf)'					# INFORMATION
alias j='eval $(cat $DOTFILE_DIR/lists/commands.txt | fzf)'                     # GENERAL COMMANDS
alias g='eval $(cat $DOTFILE_DIR/lists/git.txt | fzf)'	                        # GIT
alias gg='eval $(cat $DOTFILE_DIR/lists/gcloud.txt | fzf)'                      # GCLOUD
alias n='eval $(cat $DOTFILE_DIR/lists/npm.txt | fzf)'	                        # NPM
alias nn='vim -c "set filetype=markdown" $DOTFILE_DIR/lists/notes.txt'          # NOTES
alias mark='MARK=$(echo $PWD) && echo "$MARK" | pbcopy'                         # MARK DIR
alias recall='echo $MARK | xargs -I {} echo {} && echo $MARK | pbcopy'			# RECALL DIR
alias o='[ -f "pom.xml" ] && idea . || { [ -f "requirements.txt" ] } && pycharm . || code .'   # OPEN PROJECT
alias p='eval $(cat $DOTFILE_DIR/lists/python.txt | fzf)'                                      # PYTHON
alias l='eval $(cat $DOTFILE_DIR/lists/web-pages.txt | fzf)'			                       # WEBPAGES
alias ll='$DOTFILE_DIR/scripts/ingredients.sh $(cat $DOTFILE_DIR/lists/ingredients.txt | fzf)' # INGREDIENTS
alias f='cd $(find ~/Developer -maxdepth 2 -type d \( -name tools \) -prune -o -type d | fzf)' # SEARCH DIRECTORY
alias s='cd $(find . -type d \( -name .angular -o -name .git -o -name node_modules \) -prune -o -type d | fzf)' # SELECT DIRECTORY
alias ss='eval $(cat $DOTFILE_DIR/lists/spotify.txt | fzf)'			                           # SPOTIFY
alias t='vim -c "set filetype=markdown" $DOTFILE_DIR/lists/tasks.txt'                          # TASKS
alias u='source ~/.zshrc'                                                                      # SOURCE ZSHRC FILE
alias v='eval $(cat $DOTFILE_DIR/lists/config.txt | fzf)'                                      # CONFIG
alias ö='open -a ChatGPT'                                                                      # CHATGPT
alias öö='open -a Spotify'                                                                     # OPEN SPOTIFY
alias ..='cd ..'                                                                               # RETURN TO PREVIOUS DIR
alias x='cd $(find ~/Documents -maxdepth 3 -type d | fzf)'                 					   # SEARCH DOCUMENTS
alias xx='vim $(find ~/documents -type d \( -name node_modules -o -name .angular -o -name target -o -name .git \) -prune -o -type f ! -name "*.png" | fzf --preview="bat --color=always {}")' # OPEN FILE
